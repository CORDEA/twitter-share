{
    "name": "Share",
    "version": "0.0.1",
    "description": "Share",
    "permissions" : [
        "tabs",
        "<all_urls>"
    ],
    "browser_action": {
        "default_title": "Share",
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://unpkg.com https://cdn.jsdelivr.net; object-src 'self'",
    "manifest_version": 2
}
